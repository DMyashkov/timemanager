{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "components/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src/**/*"
  ],
      "exclude": ["node_modules"] // Optional, but should not exclude your file

}
